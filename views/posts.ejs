<%- include('header') %>

  <div class="container">
    <% for(var i=0;i<posts.length;i++) { %>
      <div class="media">
        <div class="media-left media-middle">
          <a href="#">
            <img class="media-object" src="/<%= posts[i].user %>.png" alt="<%= posts[i].user %>" width="100" height="100">
          </a>
        </div>
        <div class="media-body">
          <h4 class="media-heading"><%= posts[i].tit %></h4>
          <%= posts[i].content %>
        </div>
      </div>
      <% } %>
  </div>

  <%- include('footer') %>